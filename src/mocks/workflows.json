  {
    "id": "workflow-002-risk-escalation",
    "name": "Risk Detection & Escalation",
    "nodes": [
      {
        "id": "source-notes",
        "type": "trigger",
        "label": "Notes / Transcripts",
        "description": "Triggered when new notes are added",
        "instanceId": "source-notes-q7r8s9t0"
      },
      {
        "id": "ai-risks",
        "type": "ai",
        "label": "Extract Risks & Decisions",
        "description": "Find key risks and decisions",
        "instanceId": "ai-risks-u1v2w3x4",
        "config": {
          "riskThreshold": "medium",
          "includeDecisions": true
        }
      },
      {
        "id": "logic-branch",
        "type": "branch",
        "label": "Condition (If/Else)",
        "description": "Branch workflow based on conditions",
        "instanceId": "logic-branch-y5z6a7b8",
        "config": {
          "condition": "risk.severity === 'High'",
          "operator": "equals"
        },
        "branches": {
          "true": [
            {
              "id": "action-email",
              "type": "action",
              "label": "Send Email",
              "description": "Send summary via email",
              "instanceId": "action-email-c9d0e1f2",
              "config": {
                "to": "leadership@company.com",
                "subject": "ðŸš¨ High-Severity Risk Detected",
                "template": "risk-alert"
              }
            },
            {
              "id": "action-slack-urgent",
              "type": "action",
              "label": "Send to Slack",
              "description": "Post to Slack channel",
              "instanceId": "action-slack-g3h4i5j6",
              "config": {
                "channel": "#incidents",
                "message": "Urgent: High-severity risk identified",
                "priority": "high"
              }
            }
          ],
          "false": [
            {
              "id": "action-notion",
              "type": "action",
              "label": "Save to Notion",
              "description": "Add to Notion database",
              "instanceId": "action-notion-k7l8m9n0",
              "config": {
                "database": "Risk Register",
                "autoTag": true
              }
            }
          ]
        }
      }
    ],
    "isEnabled": true,
    "createdAt": "2025-01-12T10:15:00.000Z",
    "updatedAt": "2025-01-25T16:40:00.000Z"
  },
  {
    "id": "workflow-003-customer-feedback",
    "name": "Customer Feedback Processing",
    "nodes": [
      {
        "id": "source-email",
        "type": "trigger",
        "label": "Email",
        "description": "Triggered by incoming emails",
        "instanceId": "source-email-o1p2q3r4"
      },
      {
        "id": "ai-sentiment",
        "type": "ai",
        "label": "Sentiment Analysis",
        "description": "Analyze tone and sentiment",
        "instanceId": "ai-sentiment-s5t6u7v8",
        "config": {
          "detectEmotion": true,
          "urgencyDetection": true
        }
      },
      {
        "id": "logic-branch-sentiment",
        "type": "branch",
        "label": "Condition (If/Else)",
        "description": "Branch workflow based on conditions",
        "instanceId": "logic-branch-w9x0y1z2",
        "config": {
          "condition": "sentiment === 'negative'",
          "operator": "equals"
        },
        "branches": {
          "true": [
            {
              "id": "ai-summary-urgent",
              "type": "ai",
              "label": "Extract Summary",
              "description": "Generate concise summary",
              "instanceId": "ai-summary-a3b4c5d6",
              "config": {
                "length": "Short",
                "focus": "issues"
              }
            },
            {
              "id": "action-jira-support",
              "type": "action",
              "label": "Create Jira Issue",
              "description": "Create task in Jira",
              "instanceId": "action-jira-e7f8g9h0",
              "config": {
                "project": "SUPPORT",
                "issueType": "Bug",
                "priority": "High",
                "labels": ["customer-escalation"]
              }
            }
          ],
          "false": [
            {
              "id": "action-slack-feedback",
              "type": "action",
              "label": "Send to Slack",
              "description": "Post to Slack channel",
              "instanceId": "action-slack-i1j2k3l4",
              "config": {
                "channel": "#customer-feedback",
                "message": "New positive feedback received"
              }
            }
          ]
        }
      }
    ],
    "isEnabled": true,
    "createdAt": "2025-01-15T14:00:00.000Z",
    "updatedAt": "2025-01-26T09:30:00.000Z"
  },
