  {
    "id": "workflow-005-compliance-check",
    "name": "Compliance & Security Review",
    "nodes": [
      {
        "id": "source-calendar",
        "type": "trigger",
        "label": "Calendar Event",
        "description": "Triggered by calendar events",
        "instanceId": "source-calendar-g5h6i7j8"
      },
      {
        "id": "ai-custom-compliance",
        "type": "ai",
        "label": "Custom AI Prompt",
        "description": "Run custom prompt",
        "instanceId": "ai-custom-k9l0m1n2",
        "config": {
          "prompt": "Identify any compliance, security, or privacy concerns mentioned in the meeting",
          "outputFormat": "structured"
        }
      },
      {
        "id": "logic-branch-compliance",
        "type": "branch",
        "label": "Condition (If/Else)",
        "description": "Branch workflow based on conditions",
        "instanceId": "logic-branch-o3p4q5r6",
        "config": {
          "condition": "concerns.length > 0",
          "operator": "greaterThan"
        },
        "branches": {
          "true": [
            {
              "id": "action-jira-compliance",
              "type": "action",
              "label": "Create Jira Issue",
              "description": "Create task in Jira",
              "instanceId": "action-jira-s7t8u9v0",
              "config": {
                "project": "COMPLIANCE",
                "issueType": "Task",
                "priority": "High",
                "watchers": ["compliance-team"]
              }
            },
            {
              "id": "action-email-compliance",
              "type": "action",
              "label": "Send Email",
              "description": "Send summary via email",
              "instanceId": "action-email-w1x2y3z4",
              "config": {
                "to": "compliance@company.com",
                "subject": "Compliance Review Required",
                "cc": ["legal@company.com"]
              }
            }
          ],
          "false": [
            {
              "id": "action-webhook-log",
              "type": "action",
              "label": "Webhook",
              "description": "Send data to webhook",
              "instanceId": "action-webhook-a5b6c7d8",
              "config": {
                "url": "https://api.company.com/compliance/log",
                "method": "POST"
              }
            }
          ]
        }
      }
    ],
    "isEnabled": true,
    "createdAt": "2025-01-20T09:45:00.000Z",
    "updatedAt": "2025-01-24T13:10:00.000Z"
  }
